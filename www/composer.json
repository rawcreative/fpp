{
	"name": "falconchristmas/fpp",
	"description": "Falcon Player",
	"keywords": ["falcon", "raspberry pi", "falcon player", "falcon pi player"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.0.*",
	  	"fpp/fpp-installer": "dev-master",
		"clue/socket-raw": "dev-master",
		"brainboxlabs/brain-socket": "dev-master",
		"jenssegers/model": "dev-master",
		"jamesmoss/flywheel": "dev-master",
		"mrrio/shellwrap": "dev-master",
	 	"laravelcollective/html": "~5.0",

	  	
		"league/csv": "~7.0@dev",
		"barryvdh/laravel-debugbar": "~2.0@dev",
		"orchestra/asset": "3.0.*",
		"league/fractal": "0.12.*@dev",
	    "fpp/big-green-button": "dev-master",
	    "fpp/menu": "dev-master"


	},
	"require-dev": {
	  	"way/generators": "~3.0",
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"FPP\\": "app/"
		},
	  "files" : [
		"app/helpers.php"
	  ]
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"repositories": [
	  {
		"type": "vcs",
		"url": "https://github.com/rawcreative/fpp-installer"
	  },
	  {
		"type": "vcs",
		"url": "https://github.com/rawcreative/big-green-button"
	  },
	  {
		"type": "vcs",
		"url": "https://github.com/rawcreative/fpp-menu.git"
	  }
	],
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
